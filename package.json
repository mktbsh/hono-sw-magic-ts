{
  "name": "hono-sw-magic-ts",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:sw": "bun build ./src/server/sw.ts --outdir ./public --format esm --sourcemap=inline --watch",
    "dev:vite": "vite",
    "build:sw": "bun build ./src/server/sw.ts --outdir ./public --format esm --minify --sourcemap=none",
    "build:vite": "tsc && vite build",
    "build": "run-s build:*",
    "preview": "vite preview"
  },
  "dependencies": {
    "hono": "^4.2.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
